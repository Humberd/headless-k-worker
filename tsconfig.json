{
  "compilerOptions": {
    "incremental": true,
    "tsBuildInfoFile": "./.buildcache/files",
    "outDir": "./dist",
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es2017",
    "lib": ["es2018"],
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "typeRoots": [
      "./node_modules/@types"
    ],
    "forceConsistentCasingInFileNames": true
  },
  "exclude": [
    "node_modules"
  ]
}
